<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <!-- Background gradient circle -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="robotGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4facfe;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00f2fe;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main background -->
  <circle cx="16" cy="16" r="15" fill="url(#bgGradient)" stroke="#ffffff" stroke-width="1"/>
  
  <!-- Robot head -->
  <rect x="9" y="8" width="14" height="11" rx="3" fill="url(#robotGradient)" stroke="#ffffff" stroke-width="0.5"/>
  
  <!-- Eyes -->
  <ellipse cx="12.5" cy="12.5" rx="2" ry="1.5" fill="#ff6b6b"/>
  <ellipse cx="19.5" cy="12.5" rx="2" ry="1.5" fill="#ff6b6b"/>
  <circle cx="12.5" cy="12.5" r="1" fill="#ffffff"/>
  <circle cx="19.5" cy="12.5" r="1" fill="#ffffff"/>
  
  <!-- Digital mouth -->
  <rect x="13" y="15" width="6" height="2.5" rx="1.25" fill="#000000"/>
  <rect x="13.5" y="15.5" width="1" height="1.5" fill="#00ff88"/>
  <rect x="15" y="15.5" width="2" height="1.5" fill="#00ff88"/>
  <rect x="17.5" y="15.5" width="1" height="1.5" fill="#00ff88"/>
  
  <!-- Antennas -->
  <line x1="11" y1="8" x2="11" y2="5" stroke="#ffffff" stroke-width="1" stroke-linecap="round"/>
  <line x1="21" y1="8" x2="21" y2="5" stroke="#ffffff" stroke-width="1" stroke-linecap="round"/>
  <circle cx="11" cy="5" r="1.25" fill="#ff6b6b"/>
  <circle cx="21" cy="5" r="1.25" fill="#ff6b6b"/>
  
  <!-- Body -->
  <rect x="10" y="19" width="12" height="9" rx="2" fill="url(#robotGradient)" stroke="#ffffff" stroke-width="0.5"/>
  
  <!-- Control panel -->
  <rect x="12" y="21" width="8" height="5" rx="1.5" fill="#1a1a2e"/>
  <circle cx="16" cy="23.5" r="1.5" fill="#ff6b6b"/>
  <circle cx="16" cy="23.5" r="0.75" fill="#ffffff"/>
  
  <!-- Alpha symbol -->
  <text x="16" y="25" font-family="Arial, sans-serif" font-size="5" font-weight="bold" text-anchor="middle" fill="#ffffff">Î±</text>
</svg>